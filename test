local Slider = MainTab:CreateSlider({
   Name = "uh hp safe location",
   Range = {0, 100},
   Increment = 10,
   Suffix = "Bananas",
   CurrentValue = 30,
   Flag = "Slider1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Value)
   local player = game.Players.LocalPlayer
local character = player.Character
local humanoid = character:WaitForChild("Humanoid")
local savedPosition = nil

local function teleportToLocation(position)
  character.HumanoidRootPart.CFrame = CFrame.new(position)
end

local function checkHealth()
  if humanoid.Health <= (Value) then
    savedPosition = character.HumanoidRootPart.CFrame.Position
    for i = 1, 5 do
      teleportToLocation(Vector3.new(1018, 442, -692))
      wait(0.2)
    end
    wait(20)
    if savedPosition then
      teleportToLocation(savedPosition)
      savedPosition = nil
    end
  end
end

while true do
  wait(0.1)
  checkHealth()
end
   end,
})

local Dropdown = MainTab:CreateDropdown({
   Name = "Dropdown Example",
   Options = {"flyhigh","haha","mountain"},
   CurrentOption = {"flyhigh"},
   MultipleOptions = false,
   Flag = "Dropdown1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
   Callback = function(Option)
local function checkText(textToCheck, textToScriptMapping)
  for targetText, scriptToExecute in pairs(textToScriptMapping) do
    if textToCheck == targetText then
      scriptToExecute() -- Call the function directly
      return -- Stop checking after a match is found
    end
  end
end

-- Example usage:
local textToScriptMapping = {
  ["mountain"] = function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(520, 440, 293))
  end,
  ["haha"] = function()
    game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(520, 1000, 100))
  end,
  ["flyhigh"] = function()
game.Players.LocalPlayer.Character:SetPrimaryPartCFrame(CFrame.new(0, 1000, 0))
  end,
}

-- Get the text input from the user (replace with your actual input method)
local userInput = "(Option)"

checkText(userInput, textToScriptMapping)
      end,
})
